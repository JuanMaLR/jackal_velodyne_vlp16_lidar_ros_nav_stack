<launch>

    <!-- Run the map server -->
    <arg name="map_file" default="$(find jackal_velodyne_vlp16_lidar_ros_nav_stack)/maps/mymap.yaml"/>
    <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />
    
    <!--- Run AMCL -->
    <include file="$(find jackal_velodyne_vlp16_lidar_ros_nav_stack)/launch/nav_stack/amcl.launch"/>
    
    <!--- Run Move Base -->
    <include file="$(find jackal_velodyne_vlp16_lidar_ros_nav_stack)/launch/nav_stack/with_map_move_base.launch"/>

</launch>